#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,r=0,re;
    scanf("%d",&n);
    while(n>0){
        re=n%10;
        r=(r*10)+re;
        n=n/10;
    }
    printf("%d",r);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
