#include<bits/stdc++.h>
using namespace std;
int main(){
    int n,r,f=0;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];}
        sort(arr,arr+n);
            int y=arr[0];
            for(int i=0;i<n;i++){
                if(arr[0]<arr[i]){
                r=arr[i];
                f=1;
                break;
                
                }
                
                
            
            }
            
if(f==0)
cout<<"NO";
else
cout<<r;
        
        return 0;
}
